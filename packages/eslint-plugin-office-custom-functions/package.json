{
  "name": "eslint-plugin-office-custom-functions",
  "version": "0.0.0",
  "description": "eslint plugin for custom functions",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Artur Tarasenko (artarase)",
  "main": "lib/",
  "scripts": {
    "build": "babel --extensions .js,.ts src --out-dir lib --copy-files",
    "postbuild": "rimraf lib/__tests__ lib/**/__tests__",
    "lint": "eslint . --ignore-pattern '!.eslintrc.js' --ext js,ts",
    "prepack": "yarn build",
    "prettylint": "prettylint docs/**/*.md README.md package.json",
    "test": "jest",
    "tools:generate-rules-table": "ts-node -T tools/generate-rules-table",
    "typecheck": "tsc -p ."
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "~2.32.0",
    "requireindex": "~1.1.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~2.32.0",
    "@typescript-eslint/parser": "~2.32.0",
    "eslint": "~3.9.1",
    "mocha": "^8.0.1",
    "typescript": "^3.9.6",
    "jest": "^25.2.0",
    "jest-runner-eslint": "^0.10.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC"
}
