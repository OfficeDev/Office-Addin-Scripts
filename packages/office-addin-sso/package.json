{
  "name": "office-addin-sso",
  "version": "1.0.2",
  "description": "Configure SSO for Office Addin project and provide SSO authentication service.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\" \"npm run copy\"",
    "cli": "node lib/cli.js",
    "copy": "cpx \"src/scripts/**/*.*\" lib/scripts && cpx \"src/views/**/*.*\" lib/views",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-sso": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "form-urlencoded": "3.0.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/jquery": "^3.3.31",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "1.7.35",
    "concurrently": "^4.1.2",
    "cpx": "1.5.0",
    "mocha": "^5.2.0",
    "office-addin-cli": "^1.0.6",
    "office-addin-dev-certs": "^1.4.13",
    "office-addin-manifest": "^1.4.15",
    "office-addin-test-helpers": "^1.0.16",
    "rimraf": "2.6.2",
    "ts-node": "^8.5.4",
    "typescript": "2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
