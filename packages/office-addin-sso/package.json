{
  "name": "office-addin-sso",
  "version": "1.3.0",
  "description": "Configure SSO for Office Addin project and provide SSO authentication service.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\" \"npm run copy\"",
    "cli": "node lib/cli.js",
    "copy": "copyfiles \"src/scripts/*.*\" -u 2 lib/scripts && copyfiles \"src/views/*.*\" -u 2 lib/views",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-sso": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.3",
    "form-urlencoded": "3.0.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.1",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.1.4",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.1",
    "office-addin-lint": "^2.1.0",
    "office-addin-usage-data": "^1.5.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@types/express": "4.17.13",
    "@types/jquery": "^3.3.34",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "1.7.35",
    "concurrently": "^6.2.2",
    "copyfiles": "2.4.1",
    "mocha": "^9.1.1",
    "office-addin-cli": "^1.4.0",
    "office-addin-dev-certs": "^1.8.0",
    "office-addin-manifest": "^1.9.0",
    "office-addin-test-helpers": "^1.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config",
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
