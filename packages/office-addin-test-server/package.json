{
    "name": "office-addin-test-server",
    "version": "1.0.13",
    "description": "Creates a local web server for testing Office Add-ins and receiving test results.",
    "main": "./lib/main.js",
    "scripts": {
        "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\"",
        "cli": "node lib/cli.js",
        "test": "mocha -r ts-node/register test/**/*.ts",
        "watch": "rimraf lib && concurrently \"tsc -p tsconfig.json -w\""
    },
    "author": "Office Dev",
    "license": "MIT",
    "bin": {
        "office-addin-test-server": "./cli.js"
    },
    "keywords": [
        "Office",
        "Office Add-in"
    ],
    "dependencies": {
        "commander": "^2.19.0",
        "cors": "^2.8.5",
        "es6-promise": "^4.2.6",
        "express": "^4.16.4",
        "office-addin-cli": "^0.2.18",
        "office-addin-dev-certs": "^1.4.7"
    },
    "devDependencies": {
        "@types/cors": "^2.8.4",
        "@types/es6-collections": "^0.5.31",
        "@types/es6-promise": "0.0.32",
        "@types/express": "^4.16.1",
        "@types/mocha": "^5.2.5",
        "@types/node": "^11.9.4",
        "concurrently": "^4.1.0",
        "mocha": "^5.2.0",
        "rimraf": "^2.6.3",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
    },
    "bugs": {
        "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
    }
}
