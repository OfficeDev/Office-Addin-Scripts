{
  "name": "office-addin-cli",
  "version": "1.0.12",
  "description": "A command-line interface for Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-cli": "./cli.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "commander": "^2.20.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.34",
    "@types/node-fetch": "^2.5.5",
    "@types/sinon": "^7.5.2",
    "concurrently": "^3.5.0",
    "mocha": "^7.1.1",
    "rimraf": "^2.7.1",
    "sinon": "^7.5.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
