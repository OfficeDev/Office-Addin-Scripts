{
  "name": "office-addin-cli",
  "version": "0.2.5",
  "description": "A command-line interface for Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-cli": "./cli.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "commander": "^2.19.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.6",
    "@types/sinon": "^7.0.10",
    "concurrently": "^3.5.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.2.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
