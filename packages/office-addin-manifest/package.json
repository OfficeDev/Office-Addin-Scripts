  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^6.2.0",
    "node-fetch": "^2.6.1",
    "office-addin-cli": "^1.3.3",
    "office-addin-usage-data": "^1.4.4",
    "path": "^0.12.7",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.17.2",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^9.4.4",
    "@types/xml2js": "^0.4.5",
    "concurrently": "^6.2.2",
    "copy-dir": "^0.4.0",
    "fs-extra": "^7.0.0",
    "mocha": "^9.1.1",
    "office-addin-lint": "^1.4.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "validator": "^13.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
