{
  "name": "custom-functions-metadata",
  "version": "1.0.20",
  "description": "Generate metadata for Excel Custom Functions.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/src/*.ts",
    "package": "node ./lib/cli.js generate"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "custom-functions-metadata": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in",
    "Excel Custom Funtions"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "commander": "^2.19.0",
    "nconf": "^0.10.0",
    "office-addin-cli": "^0.2.18",
    "optimist": "^0.6.1",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/commander": "^2.12.2",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.6",
    "@types/node-fetch": "^2.5.4",
    "@types/xregexp": "^3.0.30",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0",
    "office-addin-lint": "^1.0.15",
    "office-addin-prettier-config": "^1.0.11",
    "rimraf": "^2.7.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config"
}
