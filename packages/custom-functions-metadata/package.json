{
  "name": "custom-functions-metadata",
  "version": "2.1.5",
  "description": "Generate metadata for Excel Custom Functions.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.json",
    "cli": "node lib/cli.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "package": "node ./lib/cli.js generate",
    "prettier": "office-addin-lint prettier",
    "start": "rimraf lib && concurrently \"npm run watch\"",
    "test": "mocha -r ts-node/register test/src/*.ts",
    "watch": "tsc -p tsconfig.json -w"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "custom-functions-metadata": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in",
    "Excel Custom Funtions"
  ],
  "dependencies": {
    "assert": "^1.5.0",
    "commander": "^13.0.0",
    "nconf": "^0.12.0",
    "office-addin-usage-data": "^2.0.5",
    "xregexp": "^4.3.0"
  },
  "devDependencies": {
    "@types/assert": "^1.4.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^14.17.2",
    "@types/node-fetch": "^2.5.10",
    "@types/xregexp": "^3.0.30",
    "concurrently": "^9.0.0",
    "mocha": "^11.0.1",
    "office-addin-lint": "^3.0.5",
    "office-addin-prettier-config": "^2.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config",
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
