{
  "name": "custom-functions-metadata",
  "version": "1.0.1",
  "description": "Generate metadata for Excel Custom Functions.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/src/*.ts",
    "package": "node ./lib/cli.js generate"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "custom-functions-metadata": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in",
    "Excel Custom Funtions"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "commander": "^2.19.0",
    "nconf": "^0.10.0",
    "office-addin-cli": "^0.2.5",
    "optimist": "^0.6.1",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "@types/assert": "^1.4.2",
    "@types/commander": "^2.12.2",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/xregexp": "^3.0.29",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
