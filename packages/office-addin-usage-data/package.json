    "@types/node": "^14.17.2",
    "concurrently": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-office-addins": "^4.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^9.1.1",
    "office-addin-prettier-config": "^2.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-eslint": "^8.4.0"
  },
  "repository": {
    "type": "git",
