    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "concurrently": "^6.0.2",
    "mocha": "^7.1.1",
    "eslint": "7.28.0",
    "eslint-plugin-office-addins": "^0.2.2",
    "eslint-plugin-prettier": "^3.4.0",
