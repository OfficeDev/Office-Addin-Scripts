{
  "name": "office-addin-lint",
  "version": "1.0.20",
  "description": "Provide eslint and prettier integration",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "test": "mocha -r ts-node/register test/src/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-lint": "lib/cli.js"
  },
  "config": {
    "lint_files": "src/**/*.{ts,tsx,js,jsx}"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "commander": "^2.20.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "office-addin-usage-data": "^1.1.2",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/commander": "^2.12.2",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.4",
    "assert": "^1.4.1",
    "concurrently": "^3.6.1",
    "mocha": "^7.1.1",
    "office-addin-cli": "^0.2.18",
    "rimraf": "^2.7.1",
    "ts-node": "^8.5.4",
    "tslint": "^6.1.0",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config",
  "eslintConfig": {
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-eval": "error",
      "no-delete-var": "warn",
      "no-octal": "warn",
      "no-inner-declarations": "warn"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "extends": [
      "prettier",
      "prettier/@typescript-eslint"
    ]
  },
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
