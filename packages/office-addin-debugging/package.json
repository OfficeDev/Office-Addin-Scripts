{
  "name": "office-addin-debugging",
  "version": "4.3.7",
  "description": "For debugging Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\"",
    "cli": "node lib/cli.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "node-debugger": "office-addin-node-debugger",
    "prettier": "office-addin-lint prettier",
    "start": "rimraf lib && concurrently \"npm run watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "watch": "tsc -p tsconfig.json -w"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-debugging": "./cli.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^6.2.0",
    "node-fetch": "^3.1.1",
    "office-addin-cli": "^1.3.4",
    "office-addin-dev-certs": "^1.7.6",
    "office-addin-dev-settings": "^1.10.6",
    "office-addin-manifest": "^1.7.6",
    "office-addin-node-debugger": "^0.6.5",
    "office-addin-usage-data": "^1.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^14.17.2",
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^6.0.4",
    "concurrently": "^6.2.2",
    "express": "^4.16.4",
    "mocha": "^9.1.1",
    "office-addin-lint": "^1.4.5",
    "rimraf": "^2.7.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "ws": "^7.4.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "prettier": "office-addin-prettier-config",
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
