{
  "name": "office-addin-debugging",
  "version": "4.1.2",
  "description": "For debugging Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "node-debugger": "office-addin-node-debugger"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-debugging": "./cli.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^6.2.0",
    "node-fetch": "^2.6.1",
    "office-addin-cli": "^1.0.19",
    "office-addin-dev-certs": "^1.5.14",
    "office-addin-dev-settings": "^1.8.18",
    "office-addin-manifest": "^1.5.18",
    "office-addin-node-debugger": "^0.4.40",
    "office-addin-usage-data": "^1.0.25"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.17.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.12.34",
    "@types/node-fetch": "^2.5.5",
    "@types/ws": "^6.0.4",
    "concurrently": "^6.0.0",
    "express": "^4.16.4",
    "mocha": "^7.1.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "ws": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  },
  "gitHead": "dffbcd4d1397534105b98fb13ba0fb1c3aba2dea"
}
