{
  "name": "office-addin-debugging",
  "version": "0.0.1",
  "description": "For debugging Office Add-ins.",
  "main": "./lib/debugging.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "node-debugger": "office-addin-node-debugger"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-debugging": "./lib/debugging.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^2.9.0",
    "node-fetch": "^2.2.0",
    "office-addin-dev-settings": "^1.0.0",
    "office-addin-manifest": "^1.0.0",
    "office-addin-node-debugger": "^0.1.0"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/node": "^10.5.7",
    "@types/node-fetch": "^2.1.2",
    "concurrently": "^3.5.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
