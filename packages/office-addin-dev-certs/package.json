{
  "name": "office-addin-dev-certs",
  "version": "1.5.21",
  "description": "For managing certificates when developing Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-dev-certs": "./cli.js"
  },
  "keywords": [
    "Office",
    "add-in",
    "localhost",
    "SSL",
    "https",
    "certificates",
    "keys",
    "cert",
    "development",
    "secure",
    "server"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^6.2.0",
    "fs-extra": "^7.0.1",
    "mkcert": "^1.4.0",
    "office-addin-cli": "^1.1.3",
    "office-addin-usage-data": "^1.2.6"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/fs-extra": "^5.1.0",
    "@types/mkcert": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.34",
    "concurrently": "^6.0.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
