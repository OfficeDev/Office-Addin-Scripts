{
  "name": "office-addin-dev-certs",
  "version": "1.4.7",
  "description": "For managing certificates when developing Office Add-ins.",
  "main": "./lib/main.js",
  "scripts": {
    "cli": "node lib/cli.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-dev-certs": "./cli.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "mkcert": "^1.2.0",
    "office-addin-cli": "^0.2.18"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/fs-extra": "^5.0.5",
    "@types/mkcert": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "concurrently": "^3.5.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.2.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
