{
  "name": "office-addin-start-debug",
  "version": "0.0.3",
  "description": "This package enables the required functionality when pressing F5, i.e., this package is referenced in the launch.json in VS Code and then handles all the required steps in order to start a debugging session.",
  "main": "./lib/start-debugging.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "node-debugger": "office-addin-node-debugger"
  },
  "author": "",
  "license": "MIT",
  "bin": {
    "office-addin-start-debug": "./lib/start-debugging.js"
  },
  "keywords": [
    "Office"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "node-fetch": "^2.2.0",
    "commander": "^2.9.0",
    "office-addin-node-debugger": "^0.0.2"
  },
  "devDependencies": {
    "@types/node": "^10.5.7",
    "@types/node-fetch": "^2.1.2",
    "@types/es6-promise": "0.0.32",
    "@types/commander": "^2.3.31",
    "rimraf": "^2.6.1",
    "concurrently": "^3.5.0"
  }
}
