{
  "name": "office-addin-node-debugger",
  "version": "0.0.2",
  "description": "Files for enabling office add-in web debugging using Node and VS Code.",
  "main": "./lib/debugger.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "bin": {
    "office-addin-node-debugger": "./lib/debugger.js"
  },
  "keywords": [
    "Office",
    "Node Debugger"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "node-fetch": "^2.2.0",
    "ws": "^6.0.0",
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.7",
    "@types/ws": "^5.1.2",
    "@types/node-fetch": "^2.1.2",
    "@types/es6-promise": "0.0.32",
    "@types/commander": "^2.3.31",
    "rimraf": "^2.6.1",
    "concurrently": "^3.5.0"
  }
}
